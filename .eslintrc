{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "semi": 2,
    "quotes": ["error", "single"],
    "eqeqeq": 2,
    "indent": ["error", 2],
    "no-console": "off",
    "no-undef": [2],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ]
  }
}
